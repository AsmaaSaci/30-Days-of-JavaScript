const maxNumEdgesToRemove=(e,t)=>{const a=(e,t)=>e.parent[t]==t?t:(e.parent[t]=a(e,e.parent[t]),a(e,e.parent[t])),r=(e,t,r)=>{let l=a(e,t),n=a(e,r);return l!=n?(e.size[l]+=e.size[n],e.size[n]=0,e.parent[n]=l,e.totalSize++,1):0},l={parent:new Array(e+1).fill(e).map(((e,t)=>t)),size:new Array(e+1).fill(1),totalSize:1},n={parent:new Array(e+1).fill(e).map(((e,t)=>t)),size:new Array(e+1).fill(1),totalSize:1};let i=0;for(let[e,a,o]of t)3==e&&(i+=Math.max(r(l,a,o),r(n,a,o)));for(let[e,a,o]of t)i+=r(1==e?l:n,a,o);return l.totalSize!=e||n.totalSize!=e?-1:t.length-i};
