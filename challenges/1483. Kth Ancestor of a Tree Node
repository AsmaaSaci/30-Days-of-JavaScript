function LCA(n, parent) {

    const LOG = 31;
    const up = Array.from({ length: LOG }, () => Array(n).fill(-1));

    for (let i = 0; i < n; ++i) 
        up[0][i] = parent[i];

    for (let j = 1; j < LOG; ++j)
        for (let i = 0; i < n; ++i)
            if (up[j - 1][i] !== -1)
                up[j][i] = up[j - 1][up[j - 1][i]];

  return up;
}

/**
 * @param {number} n
 * @param {number[]} parent
 */
var TreeAncestor = function(n, parent) {
    this.up = LCA(n, parent);
};

/** 
 * @param {number} node 
 * @param {number} k
 * @return {number}
 */
TreeAncestor.prototype.getKthAncestor = function(node, k) {
    const LOG = 31;

    for (let j = 0; j < LOG; ++j) {
        if ((k >> j) & 1) {
            node = this.up[j][node];
            
            if (node === -1)    
                break;
        }
    }

    return node;
};

/** 
 * Your TreeAncestor object will be instantiated and called as such:
 * var obj = new TreeAncestor(n, parent)
 * var param_1 = obj.getKthAncestor(node,k)
 */
