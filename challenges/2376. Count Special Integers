const countSpecialNumbers = (N) => countNumberWithoutRepeatedDigit(N);

let res, n;
const countNumberWithoutRepeatedDigit = (N) => {
    n = N;
    res = 0;
    dfs(0, 1023);
    return res - 1;
};

const checkIthBit = (x, i) => x & (1 << i);

const dfs = (cur, rem) => {
    if (cur > n) return;
    res++;
    for (let i = cur == 0 ? 1 : 0; i < 10; i++) {
        if (checkIthBit(rem, i)) {
            dfs(cur * 10 + i, rem ^ (1 << i))
        }
    }
};
