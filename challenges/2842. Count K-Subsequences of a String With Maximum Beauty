const countKSubsequencesWithMaxBeauty=(t,n)=>{const o=BigInt(1000000007),e=new Map;for(const n of t)e.set(n,(e.get(n)??0)+1);if(e.size<n)return 0;let i=BigInt(1);const g=new Map;for(const t of e.values())g.set(t,(g.get(t)??0)+1);const r=[...g.entries()].sort(((t,n)=>n[0]-t[0]));for(const[t,e]of r){if(e>=n)return i*=comb(BigInt(e),BigInt(n))*modPow(BigInt(t),BigInt(n),o),Number(i%o);i*=modPow(BigInt(t),BigInt(e),o),i%=o,n-=e}},modPow=(t,n,o)=>{let e=BigInt(1);for(;n>0;)n%BigInt(2)===BigInt(1)&&(e=e*t%o),t=t*t%o,n/=BigInt(2);return e},comb=(t,n)=>{if(n>t)return BigInt(0);let o=BigInt(1);for(let e=BigInt(0);e<n;e++)o=o*(t-e)/(e+BigInt(1));return o};
