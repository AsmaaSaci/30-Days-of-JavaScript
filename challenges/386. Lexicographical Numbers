public class Solution {
    public IList<int> LexicalOrder(int n) {
        List<int> result = new List<int>();
        int current = 1;

        while (true) {
            if (!GetNumbers()) {
                break;
            }

            if (current % 10 == 9) {
                break;
            }

            current++;
        }

        return result;

        bool GetNumbers() {
            if (current > n) {
                return false;
            }

            result.Add(current);

            current *= 10;
            while (true) {
                if (!GetNumbers()) {
                    break;
                }

                if (current % 10 == 9) {
                    break;
                }

                current++;
            }
            current /= 10;

            return true;
        }
    }
}
