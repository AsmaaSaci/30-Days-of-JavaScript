const findCriticalAndPseudoCriticalEdges=(n,t)=>{for(let n=0;n<t.length;n++)t[n].push(n);t.sort(((n,t)=>n[2]-t[2]));let e=kruskalAlgo(n,t),r=[],l=[];for(let i=0;i<t.length;i++){let u=kruskalAlgo(n,t,i),f=kruskalAlgo(n,t,-1,i);e<u?r.push(t[i][3]):e==f&&l.push(t[i][3])}return[r,l]};var findParent=function(n,t){return n[t]==t?t:n[t]=findParent(n,n[t])},unionSet=function(n,t,e,r){n=findParent(e,n),t=findParent(e,t),r[n]<r[t]?e[n]=t:r[t]<r[n]?e[t]=n:(e[t]=n,r[n]++)},kruskalAlgo=function(n,t,e,r=-1){let l=new Array(n),i=new Array(n);for(let t=0;t<n;t++)l[t]=t,i[t]=0;let u=0;-1!==r&&(u+=t[r][2],unionSet(t[r][0],t[r][1],l,i));for(let n=0;n<t.length;n++){if(n==e)continue;let r=findParent(l,t[n][0]),f=findParent(l,t[n][1]),o=t[n][2];r!=f&&(unionSet(r,f,l,i),u+=o)}for(let t=0;t<n;t++){if(findParent(l,t)!=findParent(l,0))return Number.MAX_SAFE_INTEGER}return u};
