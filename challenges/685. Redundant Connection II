const findRedundantDirectedConnection=e=>{const t=e.length,n=new Map,s=new Map,o={node:1,size:0,latestEdge:[]},i={node:1,size:0};for(let e=1;e<=t;++e)s.set(e,new Set),n.set(e,new Set);for(const[t,d]of e){const e=n.get(t);e.add(d),e.size>i.size&&(i.node=t,i.size=e.size);const r=s.get(d);r.add(t),r.size>=o.size&&(o.node=d,o.size=r.size,o.latestEdge=[t,d])}const d=new Set;const r=o.size>1?o.node:i.node;if(function e(t){if(d.has(t))return!0;d.add(t);for(const s of n.get(t))if(e(s))return!0;return!1}(r))for(let t=e.length-1;t>=0;--t){const[n,s]=e[t];if(1===o.size&&d.has(s)||o.size>1&&s===r&&d.has(n))return e[t]}return o.latestEdge};
