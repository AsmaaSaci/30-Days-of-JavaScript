/**
 * @param {number[]} stones
 * @return {number}
 */
var stoneGameVIII = function (stones) {
  const n = stones.length;

  const preSum = new Array(n).fill(0);
  preSum[0] = stones[0];
  for (let i = 1; i < n; i++) {
    preSum[i] = preSum[i - 1] + stones[i];
  }

  let ans = preSum.at(-1);
  for (let i = n - 2; i >= 1; i--) {
    ans = Math.max(ans, preSum.at(i) - ans);
  }
  return ans;
};
